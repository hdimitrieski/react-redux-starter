{
  "name": "react-redux-starter",
  "version": "0.0.0",
  "description": "Starter project for creating applications with React and Redux.",
  "engines": {
    "npm": ">=3"
  },
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass client/src/ -o client/src/",
    "watch-css": "npm run build-css && node-sass client/src/ -o client/src/ --watch --recursive",
    "server": "babel-node tools/devServer.js",
    "start": "cross-env NODE_ENV=development npm-run-all -p watch-css server",
    "build": "npm-run-all build-css ",
    "lint": "eslint **/*.{js,jsx} --color",
    "test": "cross-env NODE_ENV=test babel-node tools/test.js --colors",
    "test:cover": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hdimitrieski/react-redux-starter.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "webpack",
    "boilerplate",
    "starter"
  ],
  "author": "Hristijan Dimitrieski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hdimitrieski/react-redux-starter/issues"
  },
  "homepage": "https://github.com/hdimitrieski/react-redux-starter#readme",
  "dependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-es2015": "^1.2.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chalk": "^1.1.3",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.5.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
